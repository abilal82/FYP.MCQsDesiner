
<!DOCTYPE html>
<html  lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
  
</head>
<body class="container-fluid" style="background-color: #EAEDF1;padding-top:0px;">
    <div class="navbar navbar-inverse">
        <div class="container-fluid">
            <div class="navbar-header">
                <a class="navbar-brand" href="#">Minhaj e-Exam </a>
            </div>
        </div>
    </div>


    <div style="margin-top:10px;">
        <center>
            <img role="none" src="~/images/logo/your school.png" />
            @RenderBody()
        </center>
    </div>

    <div class="container-fluid text-center" style="padding-top:15%">
        <div style="clear:both; ">
            &copy; @DateTime.Now.Year  <a href="#" target="_blank">Online Examination System</a>
        </div>
    </div>
    @Scripts.Render("~/bundles/lib")
    
    @RenderSection("scripts", required: false)


</body>

</html>
